<% include header %> <% if (entries.length) { %> <%
entries.forEach(function(entry) { %>
<div class="panel panel-default">
  <div class="panel-heading">
    <div class="text-muted pull-right"><%= entry.published %></div>
    <%= entry.title %>
  </div>
  <div class="panel-body"><%= entry.body %></div>
</div>
<% }) %> <% } else { %> 내용 없음! <a href="/new-entry">추가하기!</a>
<% } %> <% include footer %>
